<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name=" Test Suite" parallel="tests" thread-count="3">
	<test name="Google Test" verbose="4">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://www.google.com/" />
		<classes>
			<class name="test.GoogleTest" />
		</classes>
	</test>

	<test name="OpeCartTest" verbose="4">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://demo.opencart.com/index.php?route=common/home" />
		<classes>
			<class name="test.DemoCartTest" />
		</classes>
	</test>


	<test name="OrangeHRM" verbose="4">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://www.orangehrm.com/" />
		<classes>
			<class name="test.OrangeHRMTest" />
		</classes>

	</test>

	<!-- Test -->
</suite> <!-- Suite -->
